    text-align: center !important;
}

.precitana {
    display: none;
}

.precitanaimg {
    width: 25px;
    height: 19px;
    float: right;
    background-image: url("css/post0.png");
}

    /* ++++++++++++++++++++ Technologie ++++++++++++++++++++ */
#descr {
    cursor: pointer;
}

input[type="submit"] {
    cursor: pointer;
}

    /* ++++++++++++++++++++ Produkcia ++++++++++++++++++++ */
.zobraz_tovaren {
    display: block;
    padding-top: 5px;
    background-color: transparent;
    background-image: url(css/tcerna.png);
    text-align: center;
}

.produkt_description {
    display: none;
}

.titel {
    cursor: pointer;
}

#zoznam_len_s_tovarni {
    width: 700px;
    border: 1px solid #cccccc;
    border-radius: 5px 5px;
    padding: 2px;
    margin-top: 5px;
}

#zoznam_len_s_tovarni img {
    width: 12px;
    border: 1px solid #cccccc;
    padding: 1px;
    border-radius: 2px;
    vertical-align: middle;

}

#zoznam_len_s_tovarni ul {
    height: 23px;
    margin: 0px;
    padding-top: 5px;

}

#zoznam_len_s_tovarni ul li {

    display: block;
    float: left;
    height: 23px;

    padding-left: 10px;
}

#zoznam_len_s_tovarni .left_column {
    width: 180px;
    height: 100px;
    float: left;
    border-right: 1px solid #cccccc;
}

#zoznam_len_s_tovarni .right_column {
    width: 519px;
    height: 100px;
    float: left;
    position: relative;
    background-color: transparent;
    background-image: url("css/tseda.png");

}

.tovarna_obrazok {
    background-image: url(imghra/factory.png);
    width: 700px;
    height: 189px;
    position: relative;
    border: 1px solid #cccccc;
}

.heading {
    display: block;
    background-color: transparent;
    background-image: url("css/tcerna.png");
    color: #ffdb30;
    font-size: 20px;
    padding: 5px;
    text-align: center;
}

.bottom_div {
    position: absolute;
    bottom: 0;
    width: 100%;
}

html {
    background-color: #000000;
}

.best_in_place {
    cursor: pointer;
}

.inline_list ul {
    margin: 0;
    padding: 0;
    list-style-type: none;
}

.inline_list ul li {
    display: inline-block;
    width: 80px;
}

.resources img {
    width: 15px;
    border-radius: 2px 2px;
    border: 1px solid #cccccc;
}

.bottom {
    position: absolute;
    bottom: 0;
}

.subhouse_name {
    cursor: pointer;
}

.big_button {
    width: 100px;
}

.malorodni {
  color: #ff4500;
}

