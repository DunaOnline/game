.break_word{
    word-break: keep-all;
}

.grey_checkbox{
    background: url(css/tseda.png);
}
.select_2{
    width: 200px;
}
.left_align a{
    float: left;
}

.udalost {
    width: 700px;
    position: relative;
    border: 1px solid #cccccc;
    overflow: auto;
}

.udalost_list ul {
    margin: 0;
    padding: 5px 0 0 0;
    min-height: 20px;
    list-style-type: none;
    float: left;
    width: 85%;
}

.udalost_list ul li {
    display: inline-block;
    padding: 0 10px 0 10px;
}
.udalost_list ul li:first-child {
    width: 50%;
}
textarea {
    background-color: transparent;
    background-image: url(<%= asset_path 'css/tcerna.png' %>);
    color: white;
    resize:vertical;
}
textarea.vert { resize:vertical; }
textarea.noResize { resize:none; }


.img_war_mentat{
    width:130px;
    background-image: url(<%= asset_path 'imghra/warmentat.png' %>);
}

.img_no_equip {
    width: 44px;
    margin: 2px 2px 1px 2px;
    padding: 3px 5px 3px 5px;
    font-size: 11px;
    color: #fff;
    text-shadow: 0.1em 0.1em #000;
    text-align: right;
    vertical-align: middle;
    background-color: #292929;
    background-position: left;
    background-repeat: no-repeat;
    border: solid 1px #a9a9a9;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    border-radius: 2px;
}






